            <if test="riskLevel!=null and !&quot;&quot;.equals(riskLevel.trim())">AND riskLevel=#{riskLevel}</if>
            <if test="groupLeader!=null and !&quot;&quot;.equals(groupLeader.trim())">AND groupLeader LIKE '%' + #{groupLeader} + '%'</if>
