            <if test="issuer!=null and !&quot;&quot;.equals(issuer.trim())">AND issuer LIKE '%' + #{issuer} + '%'</if>
        </where>
        ORDER BY projNo,type,date,dept,team,[group],personAcc,createTime
    </select>

