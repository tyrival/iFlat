            <if test="deadline!=null and !&quot;&quot;.equals(deadline.trim())">AND deadline=#{deadline}</if>
            <if test="feedback!=null and !&quot;&quot;.equals(feedback.trim())">AND feedback LIKE '%' + #{feedback} + '%'</if>
